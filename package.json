{
  "name": "my-tender",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^2.0.3",
    "@types/bignumber.js": "^5.0.0",
    "connected-react-router": "^4.4.1",
    "prop-types": "^15.6.2",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-scripts-ts": "2.17.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "truffle-contract": "4.0.0-beta.0",
    "typeface-roboto": "0.0.54",
    "web3": "^1.0.0-beta.34"
  },
  "scripts": {
    "app-start": "react-scripts-ts start",
    "app-build": "react-scripts-ts build",
    "app-serve-build": "./node_modules/.bin/serve -s build",
    "app-test": "react-scripts-ts test --env=jsdom",
    "eth-start": "npm run eth-start-xtrerm:genache && sleep 10s && npm run eth-deploy",
    "eth-start:genache": "./node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --gasPrice 1 --acctKeys ./ganache-accounts.json",
    "eth-start-xtrerm:genache": "xterm -e './node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --acctKeys ./ganache-accounts.json' &",
    "eth-deploy": "./node_modules/.bin/truffle migrate --compile-all --reset",
    "eth-dev": "./node_modules/.bin/truffle develop",
    "eth-comp": "./node_modules/.bin/truffle compile",
    "eth-test": "./node_modules/.bin/truffle test",
    "eject": "react-scripts-ts eject",
    "postinstall": "rm -f node_modules/web3/index.d.ts"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^9.4.7",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/react": "^16.4.6",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/react-router": "^4.0.29",
    "@types/web3": "^1.0.3",
    "ganache-cli": "^6.1.6",
    "openzeppelin-solidity": "^1.12.0",
    "serve": "^9.4.2",
    "truffle": "v5.0.0-beta.0",
    "typescript": "^3.0.1"
  }
}
