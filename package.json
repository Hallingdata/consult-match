{
  "name": "consult-match",
  "version": "0.1.0",
  "private": true,
  "homepage": "http://consult-match.hallingdata.no",
  "dependencies": {
    "@material-ui/core": "^3.8.2",
    "@material-ui/icons": "^3.0.2",
    "@types/bignumber.js": "^5.0.0",
    "@types/react-router-dom": "^4.3.0",
    "connected-react-router": "^4.4.1",
    "gh-pages": "^2.0.1",
    "jshashes": "^1.0.7",
    "ramda": "^0.25.0",
    "react": "^16.4.2",
    "react-dom": "^16.4.2",
    "react-lottie": "^1.2.3",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0",
    "redux-thunk": "^2.3.0",
    "truffle-contract": "4.0.0-beta.1",
    "truffle-hdwallet-provider": "^1.0.2",
    "typeface-roboto": "0.0.54",
    "web3": "^1.0.0-beta.37",
    "jss": "^10.0.0-alpha.3"
  },
  "scripts": {
    "app-start": "react-scripts-ts start",
    "app-build": "react-scripts-ts build && echo 'consult-match.hallingdata.no' > build/CNAME",
    "app-deploy": "gh-pages -d build",
    "app-serve-build": "./node_modules/.bin/serve -s build",
    "app-test": "react-scripts-ts test --env=jsdom",
    "eth-start": "npm run eth-start-xtrerm:genache && sleep 10s && npm run eth-deploy",
    "eth-start:genache": "./node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --gasPrice 1 --acctKeys ./ganache-accounts.json",
    "eth-start-xtrerm:genache": "xterm -e './node_modules/.bin/ganache-cli -d --gasLimit 7000000000000 --acctKeys ./ganache-accounts.json' &",
    "eth-deploy": "./node_modules/.bin/truffle migrate --compile-all --reset --network development",
    "eth-deploy:ropsten": "./node_modules/.bin/truffle migrate --network ropsten",
    "eth-deploy:ropsten-infura": "./node_modules/.bin/truffle migrate --network ropsten-infura",
    "eth-dev": "rm -rf ./src/contracts/interfaces && ./node_modules/.bin/truffle develop",
    "eth-comp": "./node_modules/.bin/truffle compile",
    "eth-test": "./node_modules/.bin/truffle test",
    "eject": "react-scripts-ts eject",
    "postinstall": "rm -f node_modules/web3/index.d.ts"
  },
  "devDependencies": {
    "@types/jest": "^23.3.1",
    "@types/node": "^9.4.7",
    "@types/ramda": "github:types/npm-ramda#dist",
    "@types/react": "^16.4.14",
    "@types/react-dom": "^16.0.6",
    "@types/react-redux": "^6.0.4",
    "@types/react-router": "^4.0.29",
    "@types/web3": "^1.0.14",
    "ganache-cli": "^6.2.5",
    "openzeppelin-solidity": "^2.1.1",
    "serve": "^10.1.1",
    "truffle": "^5.0.1",
    "typescript": "^3.2.2"
  }
}
